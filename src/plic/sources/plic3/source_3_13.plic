programme pourImbriques {
    entier x ;
    entier y ;
    entier z ;

    pour x dans 1 .. 5 repeter {
        ecrire x ;

        pour y dans x .. 10 repeter {
            ecrire y ;

            pour z dans x .. y repeter {
                ecrire z ;
            }
        }
    }
}

