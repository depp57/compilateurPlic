programme nonn {
    entier x ;
    entier y ;

    x := 7 ;
    y := 2 ;

    ecrire x = y ; //0

    ecrire x < y ; //0

    ecrire x # y ; //1

    ecrire x > y ; //1

    ecrire ( x = y ) non ( x < y ) ; // 0 non 0 -> 0
    ecrire ( x = y ) non ( x # y ) ; // 0 non 1 -> 1
    ecrire ( x # y ) non ( x > y ) ; // 1 non 1 -> 0
}